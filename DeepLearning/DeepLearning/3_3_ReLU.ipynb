   "source": [
    "# ReLU\n",
    "\n",
    "import numpy as np\n",
    "import matplotlib.pylab as plt\n",
    "\n",
    "def ReLU(x):\n",
    "    return np.maximum(0,x) # maximum 함수는 두 입력 중 큰 값을 선택해 반환하는 함수다.\n",
    "\n",
    "x = np.arange(-5.0,5.0,0.1) # -5.0~ 5.0 +=0.1\n",
    "y = ReLU(x)\n",
    "\n",
    "plt.plot(x,y)\n",
    "plt.ylim(-0.1,5.0) # y축 값 \n",
    "plt.show()\n",
    "\n"
   ]
  }
 ],
